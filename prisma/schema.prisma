generator client {
  provider = "prisma-client-js"
  previewFeatures = ["referentialIntegrity"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
  relationMode = "prisma"
}

model Usuarios {
  dni       Int   @id
  apellidos String @db.VarChar(50)
  nombres   String @db.VarChar(50)
  password  String @db.VarChar(255)
  cargo     Cargos
}

model Padrones {
  REG_REGISTRO        String @id
  FEC_RESO_CU          String
  RESO_NUM            String
  DEN_GRAD            String
  APEPAT              String
  APEMAT              String
  NOMBRE              String
  FAC_NOM             String
  F_FEC_CON_FAC_ESC   String
  DIPL_FEC            String
  COD_UNIV            String
  DOCU_TIP            String
  DOCU_NUM            String
  ABRE_GYT            String
  MOD_OBT             String
  MOD_EST             String
  RESO_FEC            String
  DIPL_TIP_EMI        String
  REG_LIBRO           String
  REG_FOLIO           String
  COD_ALU             String
  dniAct              Int
  estado              Estados @default(NO)
}

enum Cargos {
  ADMIN
  GENERADOR
}

enum Estados {
  SI
  NO
}
